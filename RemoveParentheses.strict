has text
Remove Text
	RemoveParentheses("example(unwanted thing)example").Remove is "exampleexample"
	let parentheses = Count(0)
	let result = Mutable("")
	for text
		if value is "("
			parentheses.Increment
		if parentheses is 0
			result = result + value
		if value is ")"
			parentheses = parentheses - 1
	result